				<div id="back-to-top" class="back-to-top">
					<button class="btn btn-primary" title="Back to Top">
						<i class="fa fa-angle-up"></i>
						<span class="to-top-text">To Top</span>
					</button>
				</div>

			</div><!-- Container end -->
		</div><!-- Copyright end -->


