		<script>
			$(document).ready(function(){
				$("#mobile_tab_list a").click(function(){
					var target=$(this).attr("data-target");
					$("#mobile_tab_list .col").removeClass("active");
					$(this).parent().addClass("active");
					$(".common_action").hide();
					$("#"+target+"").show();
					
				})
			})
		</script>
